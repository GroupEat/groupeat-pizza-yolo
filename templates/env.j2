DEBUG=false
APP_ENV={{ app_env }}
BASE_URL={{ 'https' if ssl_enabled else 'http' }}://{{ inventory_hostname }}/api
APP_KEY={{ app_key }}
PGSQL_PASSWORD={{ staging_pgsql_password if app_env == 'staging' else production_pgsql_password }}
DEFAULT_ADMIN_PASSWORD={{ staging_default_admin_password if app_env == 'staging' else production_default_admin_password }}
MAIL_DRIVER=smtp
QUEUE_DRIVER=beanstalkd
GCM_API_KEY={{ gcm_api_key }}
APNS_CERTIFICATE_PASSPHRASE={{ apns_certificate_passphrase }}
S3_KEY={{ s3_key }}
S3_SECRET={{ s3_secret }}
